/**
 * Example GraphViz file for the generation of a visual schema.
 *
 *	GraphViz can render different graphs (run "man dot" to see) however
 *	dot would be the best for our needs.
 * 
 *		To use:
 *
 *			dot -Tpng airtest.dot -o airtest.png
 *			setsid exo-open airtest.png
 */
digraph G {
	// Settings
	graph [
		truecolor
		bgcolor="#FFFFFF00"
	]
	node [
		shape=box
		style=filled
		fillcolor="#FFFFFF"
	]
	// Our nodes / connections
	certificates -> sites;
	certificates -> jobs;
	airtests -> certificates;
	certificates_has_items -> certificates;
	certificates_has_items -> items;
	item_records -> items;
	item_records -> certificates;
}